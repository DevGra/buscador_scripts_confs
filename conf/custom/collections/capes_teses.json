    "QUERY_BUILDER":{
          "plugins" : ["bt-tooltip-errors"],
          "filters" : [
            {

              "get_from_solr_field": "_text_",
              "label": "Busca textual",
              "operators": ["contains", "not_contains"],
              "input": "text",
              "type": "string"
            },
            {
              // campo pesquisado no solr pelo getData(), manter mesmo valor de facet_field quando usado autocomplete
              "get_from_solr_field": "NM_PRODUCAO_busca",
              // label do campo de seleção do filtro
              "label": "Nome da Produção",
              // operadores logicos utilizados pelo querybuilder
              "operators": ["equal", "not_equal"],
              // função usada pelo querybuilder para construir o input, definido em bv_querybuilder.js
              "input": "autocomplete",
              // parametro extras do autocomplete
              "solr_params": {
                // parametro do solr
                "q" : "*:*",
                // parametro do solr
                "fl" : "*",
                // numero de itens que aparecem na listagem de autocomplete
                "rows": "20",
                // campo de busca da query
                "field": "NM_PRODUCAO_busca",

                // campo que é usado para gerar lista de intes facetados no auto_com
                "facet_field" : "NM_PRODUCAO_busca"

               } ,
              "type": "string"
          },
            {
              "get_from_solr_field": "DS_PALAVRA_CHAVE_busca",
              "label": "Palavra chave PT",
              "operators": ["equal", "not_equal"],
              "input": "autocomplete",
              "solr_params": {
                "q" : "*:*",
                "fl" : "*",
                "rows": "20",
                "field": "DS_PALAVRA_CHAVE_busca",
                "facet_field" : "DS_PALAVRA_CHAVE_exact"

               } ,
              "type": "string"
          },
          {
            "get_from_solr_field": "DS_RESUMO_busca",
            "label": "Resumo PT",
            "operators": ["equal", "not_equal"],
            "input": "autocomplete",
            "solr_params": {
              "q" : "*:*",
              "fl" : "*",
              "rows": "20",
              "field": "DS_RESUMO_busca",
              "facet_field" : "DS_RESUMO_str_search"

             } ,
            "type": "string"
        },
        {
          "get_from_solr_field": "DS_KEYWORD_busca",
          "label": "Keyword EN",
          "operators": ["equal", "not_equal"],
          "input": "autocomplete",
          "solr_params": {
            "q" : "*:*",
            "fl" : "*",
            "rows": "20",
            "field": "DS_KEYWORD_busca",
            "facet_field" : "DS_KEYWORD_exact"

           } ,
          "type": "string"
      },
      {
        "get_from_solr_field": "DS_ABSTRACT_busca",
        "label": "Abstract EN",
        "operators": ["equal", "not_equal"],
        "input": "autocomplete",
        "solr_params": {
          "q" : "*:*",
          "fl" : "*",
          "rows": "20",
          "field": "DS_ABSTRACT_busca",
          "facet_field" : "DS_ABSTRACT_str_search"

         } ,
        "type": "string"
    }

